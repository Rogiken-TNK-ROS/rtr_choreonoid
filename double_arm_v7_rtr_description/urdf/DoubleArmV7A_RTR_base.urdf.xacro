<?xml version="1.0"?>
<robot name="DoubleArmV7A-RTR" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find double_arm_v7_rtr_description)/urdf/common.xacro"/>
  
  <!-- == footprint frame == -->
  <link name="base_footprint"/>
  <joint name="base_footprint_to_TFRAME" type="fixed">
    <origin xyz="0 0 0.520"/>
    <parent link="base_footprint"/>
    <child link="TFRAME"/>
  </joint>
  
  <!-- == TOHOKU ARM == -->
  <xacro:include filename="$(find double_arm_v7_rtr_description)/urdf/TOHOKU_ARM.urdf.xacro"/>

  <!-- == MANI ARM == -->
  <xacro:include filename="$(find double_arm_v7_rtr_description)/urdf/MANI_ARM.urdf.xacro"/>

  <!-- == Base == -->
  <xacro:include filename="$(find double_arm_v7_rtr_description)/urdf/BASE-AGX.urdf.xacro"/>
    
</robot>
